{
   "manifest_version": 2,
   "version": "0.1",
   "name": "Jim's Tab Counter",
   "description": "Statistics on history of open tabs.",
   
   "background": {
       "scripts" : [ "background.js" ]
   },
   
   "browser_action": {
      "default_icon": "triangle.png",
      "default_title": "Tab Graph",
      "default_popup": "main.html"
   },
   "permissions": [ "storage" ]
}
